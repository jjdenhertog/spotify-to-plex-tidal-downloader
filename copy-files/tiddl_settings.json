{
  "template": {
    "track": "{artist} - {title}",
    "video": "{artist} - {title}",
    "album": "{album_artist}/{album}/{number:02d}. {title}",
    "playlist": "Playlists/{PlaylistName}"
  },
  "download": {
    "quality": "high",
